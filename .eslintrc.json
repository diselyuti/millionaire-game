{
  "extends": [
    "next/core-web-vitals",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
    "linebreak-style": "off",
    "react/require-default-props": "off",
    "no-param-reassign": ["error", {
      "props": true,
      "ignorePropertyModificationsFor": [
        "state"
      ]
    }]
  }
}
